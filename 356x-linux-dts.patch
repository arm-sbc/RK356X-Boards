+
+&i2c0 {
+	status = "okay";
+
+    hym8563: hym8563@51 {
+		compatible = "haoyu,hym8563";
+		reg = <0x51>;
+		pinctrl-names = "default";
+		irq-gpios = <&gpio0 RK_PD3 GPIO_ACTIVE_HIGH>;
